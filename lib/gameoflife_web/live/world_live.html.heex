<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
  <div class="max-w-3xl mx-auto">
    Time: <%= @t %>

    <div class="grid grid-cols-32 mt-4">
      <%= for i <- 1..32 do %>
        <%= for j <- 1..32 do %>
          <%= link to: Routes.cell_path(@socket, :index) do  %>
            <%= live_render @socket, GameoflifeWeb.CellLive, id: to_string(i) <> "-" <> to_string(j) %>
          <% end %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>